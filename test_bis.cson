  {
    id: 20101
    level: 3
    hidden: false
    parent_id: 201
    title: 'Qualified visits'
    commentary: "Are we reaching the most qualified audience to increase our potential client base?"

    datasets:
      bars_data:
        query:[
          $match:
            domain: 'ToucanTouco'
            Subcategory: $in: ['Contact Touchpoints']
            Metric_Acronym: $in: ['NB_Visits']
            Bank_Name: $in: ['BNP','HelloBank!']
            Country_Name: $in: ['France']
        # ,
        #   $group:
        #     _id:
        #       Platform_Category : "$Platform_Category"
        #     Metric_Value: $sum: "$Metric_Value"
        ,
          $project:
              Metric_Value: 1
              Bank_Name : 1
              Platform_Category : 1
              Metric_Acronym : 1
              Period : 1
        ]

      lines_data:
        query:[
          $match:
            domain: 'ToucanTouco'
            Subcategory: $in: ['Contact Touchpoints']
            Metric_Acronym: $in: ['BounceRate']
            Bank_Name: $in: ['BNP','HelloBank!']
            Country_Name: $in: ['France']
        # ,
        #   $group:
        #     _id:
        #       Platform_Category : "$Platform_Category"
        #     Metric_Value: $sum: "$Metric_Value"
        ,
          $project:
              Metric_Value: $multiply: ["$Metric_Value", 100]
              Bank_Name : 1
              Platform_Category : 1
              Metric_Acronym : 1
              Period : 1
        ]
        precision:
          value: '.2f'
        units:
          Metric_Value: [
            unit: " Visits"
            where:
              "Metric_Acronym": "NB_Visits"
          ,
            unit: " %"
            where:
              "Metric_Acronym": "BounceRate"
          ]
    chartOptions:
      chartType: 'new-barlinechart'

      bars:
        dataset: 'bars_data'
        #domain: [0, 2000000]
      label: 'Period'
      value: 'Metric_Value'
      groups:'Bank_Name'

      lines:
        dataset: 'lines_data'
        x: 'Period'
        y: 'Metric_Value'
        domain: [0, 100]
        serie: 'Bank_Name'

      legend: true

      filters:
        "upper-right":
          on: "Platform_Category"
          type: "buttons"
          datasets: ['bars_data','lines_data']
  }
  {
    id: 20202
    level: 3
    hidden: false
    parent_id: 202
    title: 'Simulator'
  }
  {
    id: 20301
    level: 3
    hidden: false
    parent_id: 203
    title: 'Product View'
    commentary: " What products are more interesting for the potential clients? (new clients and cross sell)"
    datasets:
      bars_data:
        query: [
          $match:
            domain: 'ToucanTouco'
            Subcategory: $in: ['Acquisition']
            Metric_Acronym: $in: ['NB_Visits']
            Bank_Name: $in: ["All"]
            Indicator_Group: $in: ["Product visit"]
        ,
          $group:
            _id:
              "Indicator_Group": '$Indicator_Group'
              "Period": '$Period'
            Metric_Value: $sum: "$Metric_Value"
        ,
          $project:
            Period : "$_id.Period"
            Metric_Value: 1
      ]
        # [
        #   $match:
        #     domain: 'ToucanTouco'
        #     Subcategory: $in: ['Acquisition']
        #     Metric_Acronym: $in: ['NB_Visits']
        #     Bank_Name: $in: ['BNP','HelloBank!']
        #     Country_Name: $in: ['France','Luxembourg']
        # ]
        # mutiple_queries: true
        crossfilter:
          on: 'Period'
          datasets: ["lines_data"]
      lines_data:
        query: [
          $match:
            domain: 'ToucanTouco'
            Subcategory: $in: ['Acquisition']
            Metric_Acronym: $in: ['Pct_Visits']
            Bank_Name: $in: ['BNP','HelloBank!']
            Country_Name: $in: ['France','Luxembourg']
        ,
          $group:
            _id:
              "Period": '$Period'
            Metric_Value: $sum: "$Metric_Value"
        ,
          $project:
            Period : "$_id.Period"
            Metric_Value: 1
            Indicator_Group: 1
            Bank_Name: $literal: "All"
        ]
      slave_data:
        query: [
          $match:
            domain: 'ToucanTouco'
            Subcategory: $in: ['Acquisition']
            Metric_Acronym: $in: ['NB_Visits']
            Bank_Name: $in: ['BNP','HelloBank!']
            #Country_Name: $in: ['France','Luxembourg']
        ]
    charts: [
      {
      chartType: "new-barlinechart"
      filters:
        "bottom-right":
          on: "Indicator_Group"
          type: "buttons"
          datasets: [
            'bars_data',
            'lines_data'
          ]
      bars:
        dataset:"bars_data"
      label:"Period"
      value:"Metric_Value"
      groups:"Period"
      lines:
        dataset:"lines_data"
        x: "Period"
        y: "Metric_Value"
        serie: "Period"
      crossfilter:
        on: 'Period'
        datasets: [
          'slave_data'
        ]
      }
      {
      chartType: "horizontalbarchart"
      dataset: "slave_data"
      }
    ]
  }

