          Subcategory: $in: ['Contact Touchpoints']
          Metric_Acronym: $in: ['BounceRate']
          Bank_Name: $nin: ['All']
        precision:
          value: '.2f'
        units:
          value: 'bounce rate'

  chartOptions:
    chartType: 'new-barlinechart'

    bars:
      dataset: 'bars_data'
